@command~
{
  thing t;
  int value;
  str var;
  var="TeleportRoom";
  if(StrIsCmd(COMMAND,"enter")) {
    if(CharGetFighting(EVENT_THING)==TNULL) {
      BLOCK_CMD=YES;
      value=PropertyGetInt(CODE_THING,var);
      if(value==0) {
        stop;
      }
      t=WorldOf(value);
      SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"<Flash>\n");
      ThingTo(EVENT_THING,t);
      SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"Someone is gone\?!\?\n");
      SendAction(EVENT_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"<Flash>\n");
      SendAction(EVENT_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"Someone has just arived!\n");
    }
  }
}~
P
