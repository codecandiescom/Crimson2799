@aftercommand~
{
  thing t;
  t=ThingFind(NULL,5605,TNULL,TF_OBJ_WLD,0);
  if(StrIsCmd(COMMAND,"say")) {
    if(StrIn(COMMAND,"bridge")) {
      if(BaseGetInside(t)==WorldOf(5724)) {
        SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_AUDIBLE,"^pA computer voice bleeps \"Sorry, but you are already on the Bridge\"\n");
        stop;
      }
      SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks out of the room\n");
      ThingTo(t,WorldOf(5724));
      SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks into the room\n");
      SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"^aYou zift to the Bridge\n");
    }
    else {
      if(StrIsCmd(COMMAND,"say")) {
        if(StrIn(COMMAND,"one")) {
          if(BaseGetInside(t)==WorldOf(5723)) {
            SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_AUDIBLE,"^pA computer voice bleeps \"Sorry, but you are already at Level 1\"\n");
            stop;
          }
          SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks out of the room\n");
          ThingTo(t,WorldOf(5723));
          SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks into the room\n");
          SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"^aYou zift to Level 1\n");
        }
        else {
          if(StrIsCmd(COMMAND,"say")) {
            if(StrIn(COMMAND,"two")) {
              if(BaseGetInside(t)==WorldOf(5655)) {
                SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_AUDIBLE,"^pA computer voice bleeps \"Sorry, but you are already at Level 2\"\n");
                stop;
              }
              SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks out of the room\n");
              ThingTo(t,WorldOf(5655));
              SendAction(t,TNULL,SEND_ROOM|SEND_VISIBLE,"^aThe TurboLift whisks into the room\n");
              SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_VISIBLE,"^aYou zift to Level 2\n");
            }
            else {
              SendAction(CODE_THING,TNULL,SEND_ROOM|SEND_AUDIBLE,"^pA computer voice bleeps \"Does not compute...\"\n");
            }
            stop;
          }
        }
      }
    }
  }
}~
